{
  "name": "angular-file-picker",
  "version": "1.2.0",
  "description": "A simple file picker for Angular",
  "scripts": {
    "clean": "rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/file-picker.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/file-picker.umd.min.js",
    "dist": "node tools/dist",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run dist",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvilers/angular-file-picker.git"
  },
  "keywords": [
    "angular",
    "directive",
    "file",
    "picker"
  ],
  "author": "Fabian Vilers <fabian.vilers@dev-one.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fvilers/angular-file-picker/issues"
  },
  "homepage": "https://github.com/fvilers/angular-file-picker#readme",
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/core": "4.0.0",
    "rxjs": "5.1.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "rimraf": "2.6.1",
    "rollup": "0.41.6",
    "typescript": "2.2.0",
    "uglify-js": "2.8.21"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0"
  }
}
